            var fieldHTML = '<div><div class="row"><div class=" col-md-2 a3"><input type="text" class="form-control"  name="name"  id="inputEmail4" placeholder="Name"></div><div class=" col-md-2 a3"><input type="text" name="Designation" class="form-control" id="inputEmail4" placeholder="Designation"></div><div class=" col-md-2 a3"><select name="rw_experience" class="form-select" aria-label="Default select example" placeholder=""><option value="" translate="" class="ng-scope" selected="selected">Select</option><option value="0-1 Year" translate="" class="ng-scope">0-1 Year</option><option value="1-5 Years" translate="" class="ng-scope">1-5 Years</option><option value="5-10 Years" translate="" class="ng-scope">5-10 Years</option><option value="10-15 Years" translate="" class="ng-scope">10-15 Years</option><option value="10-15 Years" translate="" class="ng-scope">10-15 Years</option><option value="15-20 Years" translate="" class="ng-scope">15-20 Years</option><option value="20 Years &amp; Above" translate="" class="ng-scope">20 Years &amp;Above</option></select></div><div class=" col-md-2 a3"><input type="text" name="Qualification" class="form-control" id="inputEmail4" placeholder="Qualification"></div><div class=" col-md-2 a3"><input type="text" class="form-control" name="Adhar_no"   id="inputEmail4" placeholder="Adhar-card Number"></div><div class=" col-md-2"><textarea class="form-control a3" ssid="exampleFormControlTextarea1" rows="3" placeholder="Brief About Profile"></textarea></div><a class="remove_button btn12"><img src="cross.jpg" id="a2"></a></div></div>'; //New input field html 
            var x = 1; //Initial field counter is 1
            //Once add button is clicked
            $(addButton).click(function () {
              //Check maximum number of input fields
              if (x < maxField) {
                x++; //Increment field counter
                $(wrapper).append(fieldHTML); //Add field html
              }
            });
            //Once remove button is clicked
            $(wrapper).on('click', '.remove_button', function (e) {
              e.preventDefault();
              $(this).parent('div').remove(); //Remove field html
              x--; //Decrement field counter
            });
          });
        </script>

