<!-- Chart code -->
<script>
    var c1 = '<%=count%>'
    var c2 = ' <%=count1%>'
    am4core.ready(function () {
        // Themes begin

       

        // Themes end
        var chart = am4core.create("chartdiv", am4charts.PieChart3D);
        chart.hiddenState.properties.opacity = 0; // this creates initial fade-in
        chart.legend = new am4charts.Legend();
        chart.data = [{
            country: "Incubatee",
            litres: c1,
            "color": am4core.color("green")
        },
        {
            country: "EDcell-Member",
            litres: c2,
            "color": am4core.color("#yellow")
        },

        ];
        chart.innerRadius = 100;
        var series = chart.series.push(new am4charts.PieSeries3D());
        series.dataFields.value = "litres";
        series.dataFields.category = "country";
        pieSeries.slices.template.propertyFields.fill = "color";
    });
</script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
    var ctr11 = parseInt("<%=ctr%>")
    var ctr22 = parseInt("<%=ctr1%>")
    var ctr33 = parseInt("<%=ctr2%>")
    var ctr44 = parseInt("<%=ctr3%>")
    var co11 = parseInt("<%=co%>")
    var co22 = parseInt("<%=co1%>")
    var co33 = parseInt("<%=co2%>")
    var co44 = parseInt("<%=co3%>")
    var n = '<%=n%>'
    var n1 = '<%=n1%>'
    var n2 = '<%=n2%>'
    var n3 = '<%=n3%>'
    console.log(ctr1)
    google.charts.load('current', { 'packages': ['corechart'] });
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {
        var data = google.visualization.arrayToDataTable([
            ['Year', 'Incubatee', 'Member'],
            [n3, ctr44, co44],
            [n2, ctr33, co33],
            [n1, ctr22, co22],
            [n, ctr11, co11]
        ]);

        var options = {
            title: 'Past Records',
            curveType: 'function',
            legend: { position: 'bottom' }
        };

        var chart = new google.visualization.LineChart(document.getElementById('curve_chart'));

        chart.draw(data, options);
    }
</script>
